  <groupId>pl.psnc.dl.ege.webapp</groupId>
  <artifactId>ege-webclient</artifactId>
  <version>0.3</version>
  <packaging>war</packaging>
  <name>EGE Web Client</name>
  <properties>

    <project.build.sourceEncoding>
      UTF-8
    </project.build.sourceEncoding>

    <project.reporting.outputEncoding>
      UTF-8
    </project.reporting.outputEncoding>

  </properties>


  <url>/multiprojects/ege-webclient</url>
